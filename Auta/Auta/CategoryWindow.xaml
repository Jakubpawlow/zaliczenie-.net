<Window x:Class="Auta.CategoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Kategoria" Height="400" Width="600">
    <Grid>
        <StackPanel>
            <TextBlock Name="CategoryDescription" TextWrapping="Wrap" Margin="10"/>
            <ListBox Name="SubCategoryListBox" SelectionChanged="SubCategoryListBox_SelectionChanged" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
